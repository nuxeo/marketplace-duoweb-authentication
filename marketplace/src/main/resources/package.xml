<!-- See documentation at http://doc.nuxeo.com/x/HgIz -->
<package type="addon" name="nuxeo-duoweb-authentication" version="@VERSION@">
  <title>Nuxeo DuoWeb Authentication</title>
  <description>
    <p>The Nuxeo DuoWeb Authentication addon is an integration of DuoWeb access in Nuxeo login plugin and provides two-factor authentication through the Nuxeo login page.</p>
  </description>
  <home-page>http://github.com/nuxeo/nuxeo-duoweb-authentication</home-page>
  <vendor>Nuxeo</vendor>
  <installer restart="true" />
  <uninstaller restart="true" />
  <nuxeo-validation>nuxeo_certified</nuxeo-validation>
  <production-state>production_ready</production-state>
  <supported>true</supported>
  <platforms>
    <platform>server-@DISTRIB_VERSION@</platform>
    <platform>server-@DISTRIB_VERSION@-*</platform>
  </platforms>
  <license>LGPL 2.1</license>
  <license-url>http://www.gnu.org/licenses/lgpl-2.1.html</license-url>
  <visibility>PUBLIC</visibility>
</package>
